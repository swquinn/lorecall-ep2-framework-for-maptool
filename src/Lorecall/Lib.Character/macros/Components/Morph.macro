[h:tokenId=currentToken()]
[h:editMorphLink=macroLinkText("MorphEditor@Lib:Character", "none", "", tokenId)]

<section class="cs-morph">
  <label class="section-header">
    MORPH
    <a class="action edit-action" href="[r:editMorphLink]">
      <i class="icon">[r:icons.edit()]</i>
    </a>
  </label>
  <div class="layout ep2-border">
    [macro("MorphName@Lib:Character"): tokenId]
    [macro("MorphMovement@Lib:Character"): tokenId]
    [macro("MorphPools@Lib:Character"): tokenId]
    [macro("MorphDurability@Lib:Character"): tokenId]
    [macro("MorphNotes@Lib:Character"): tokenId]
  </div>
</section>
