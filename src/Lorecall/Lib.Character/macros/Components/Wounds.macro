[h:tokenId=currentToken()]
[h:wounds=character.getPropertyWithDefault("Wounds", 0)]

[h:editLink=macroLinkText("SimplePropertyEditor@this", "", "Wounds", tokenId)]
<section class="cs-wounds">
  <div class="clearfix">
    <a href="[r:editLink]" class="edit-action">
      <i class="icon">[r:icons.edit()]</i>
    </a>
  </div>

  <span class="token-property">[r:wounds]</span>
  <label>WOUNDS</label>
</section>