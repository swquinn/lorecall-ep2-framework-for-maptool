[h:traumas=getProperty("Traumas")]
[h:traumaThreshold=character.getTraumaThreshold()]

[h:editTraumas=macroLinkText("SimplePropertyEditor@this", "", "Traumas", currentToken())]
<section class="cs-trauma">

  <div class="traumas">
    <div class="clearfix">
      <a href="[r:editTraumas]" class="edit-trauma">
        <i class="icon">[r:icons.edit()]</i>
      </a>
    </div>

    <span class="token-property">[r:traumas] / [r:traumaThreshold]</span>
    <label>TRAUMAS</label>
  </div>
</section>