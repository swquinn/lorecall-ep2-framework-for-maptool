[h:tokenId=currentToken()]
[h:traumas=character.getPropertyWithDefault("Traumas", 0)]

[h:editLink=macroLinkText("SimplePropertyEditor@this", "", "Traumas", tokenId)]
<section class="cs-trauma">
  <div class="clearfix">
    <a href="[r:editLink]" class="edit-action">
      <i class="icon">[r:icons.edit()]</i>
    </a>
  </div>

  <span class="token-property">[r:traumas]</span>
  <label>TRAUMAS</label>
</section>
